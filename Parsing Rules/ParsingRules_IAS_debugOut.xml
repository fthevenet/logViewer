<?xml version="1.0" encoding="utf-16"?>
<ParsingExpressions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <TimeStamp Text="\d{2}\-\d{2}\-\d{2}\s\d{2}:\d{2}:\d{2}[\.,]\d{6}" />
  <Thread Text="(\d+):\w*" />
  <Severity Text="(?i)DEBUG|INFO|WARN|ERROR|FATAL" />
  <Logger Text="P: " />
  <Message Text=".*" />
  <Line Text="\[\d:\d{8}:&lt;time&gt;\](\s)*\[&lt;thread&gt;\]\s&lt;logger&gt;*&lt;message&gt;" />
</ParsingExpressions>